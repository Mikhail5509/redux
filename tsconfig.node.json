{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "target": "ES2022",
    "lib": ["ES2023"],
    "module": "ESNext",
    "moduleResolution": "node",
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "noEmit": false,
    "emitDeclarationOnly": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["vite.config.ts"]
}